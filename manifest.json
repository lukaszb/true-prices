{
  "manifest_version": 2,
  "name": "True Price",
  "description": "Allows to see true prices, like 200 instead of 199.99",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://www.e-piotripawel.pl/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_end"
    }
  ]
}
